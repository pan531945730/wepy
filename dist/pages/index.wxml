
	<view class="container">
        <view class="kind-list">
            <navigator url="form" class="weui-flex kind-list__item-hd">
                <view class="weui-flex__item">表单组件</view>
                <view class="right-arrow"/>
            </navigator>
            <navigator url="map" class="weui-flex kind-list__item-hd">
                <view class="weui-flex__item">地图组件</view>
                <view class="right-arrow"/>
            </navigator>
            <navigator url="canvas" class="weui-flex kind-list__item-hd">
                <view class="weui-flex__item">canvas</view>
                <view class="right-arrow"/>
            </navigator>
            <navigator url="wx-parse" class="weui-flex kind-list__item-hd">
                <view class="weui-flex__item">wxParse</view>
                <view class="right-arrow"/>
            </navigator>
            <view class="weui-flex kind-list__item-hd" bindtap="goUser" hover="hover" hover-class="hover">
                <view class="weui-flex__item">页面之间相互传值</view>
                <view class="right-arrow"/>
            </view>
            <block wx:for="{{list}}" wx:for-index="index" wx:for-item="item" wx:key="id">
                <view class="kind-list__item">
                    <view id="{{item.id}}" hover="hover" hover-class="hover" class="weui-flex kind-list__item-hd {{item.open ? 'kind-list__item-hd_show' : ''}}" bindtap="toggle" data-wpytoggle-a="{{item.id}}">
                        <view class="weui-flex__item">{{item.name}}</view>
                        <view class="right-arrow"/>
                    </view>
                    <view class="kind-list__item-bd {{item.open ? 'kind-list__item-bd_show' : ''}}">
                        <view class="weui-cells {{item.open ? 'weui-cells_show' : ''}}">
                            <navigator url="index" class="weui-cell weui-cell_access"  wx:for="{{item.pages}}" wx:for-item="page" wx:key="*this">
                                <view class="weui-cell__bd">{{page}}</view>
                                <view class="right-arrow"/>
                            </navigator>
                        </view>
                    </view>
                </view>
            </block>
        </view>
        
    <view class="loading _8bd4524"  wx:if="{{ $loading$show }}" style="{{ $loading$fixed ? 'position: fixed' : '' }}">
        <view class="spinner _8bd4524">
            <view class="spinner-container container1 _8bd4524">
                <view class="circle1 _8bd4524"/>
                <view class="circle2 _8bd4524"/>
                <view class="circle3 _8bd4524"/>
                <view class="circle4 _8bd4524"/>
            </view>
            <view class="spinner-container container2 _8bd4524">
                <view class="circle1 _8bd4524"/>
                <view class="circle2 _8bd4524"/>
                <view class="circle3 _8bd4524"/>
                <view class="circle4 _8bd4524"/>
            </view>
            <view class="spinner-container container3 _8bd4524">
                <view class="circle1 _8bd4524"/>
                <view class="circle2 _8bd4524"/>
                <view class="circle3 _8bd4524"/>
                <view class="circle4 _8bd4524"/>
            </view>
        </view>
    </view>
		
    <view class="wetoast {{$toast$reveal ? 'wetoast_show' : ''}}">
        <view class="wetoast__mask"/>
        <view class="wetoast__bd" animation="{{$toast$animationData}}">
            <block  wx:if="{{$toast$img}}">
                <view class="wetoast__bd__img {{$toast$imgClassName || ''}}">
                    <image class="wetoast__bd__img_desc" src="{{$toast$img}}" mode="{{$toast$imgMode || 'scaleToFill'}}"/>
                </view>
            </block>
            <block  wx:if="{{$toast$title}}">
                <view class="wetoast__bd__title {{$toast$titleClassName || ''}}">{{$toast$title}}</view>
            </block>
        </view>
    </view>

	</view>