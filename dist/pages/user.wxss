@import "./../npm/wepy-com-toast/toast.wxss";
.arrow{display:inline-block;height:10px;width:10px;border-width:2px 2px 0 0;border-color:#f50;border-style:solid;transform:matrix(0.71, -0.71, 0.71, 0.71, 0, 0);margin:20rpx}.lines{width:750rpx;height:50rpx;background:#fff;position:relative;margin:40rpx auto}.lines::before{content:" ";position:absolute;transform-origin:0 0;top:0;left:0;width:100%;border-top:1px solid #f50;transform:scaleY(0.5)}.lines::after{content:" ";position:absolute;transform-origin:0 0;bottom:0;left:0;width:100%;border-bottom:1px solid #f50;transform:scaleY(0.5)}.userinfo-avatar{width:40rpx;height:40rpx;border-radius:50%}.userinfo{display:flex;flex-direction:column;align-items:center;position:relative}.go-page{margin-top:40rpx}.contact{text-align:center}
